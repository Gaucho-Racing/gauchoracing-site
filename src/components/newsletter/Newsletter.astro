---
import { getServerSideProps } from './NewsletterDocs'
async function getData() {
	try {
		const response = getServerSideProps()
		const data = response
		return data
	} catch (error) {
		console.log(error)
	}
}
const data = await getData()
let contents = []
data.data.body.content.splice(1).forEach(e => {
	const temp = e.paragraph.elements[0];
	if (temp.textRun !== undefined && temp.textRun.content !== undefined) contents.push(temp.textRun.content);
})
---
<!DOCTYPE html>
<html lang="en">

<head>
	<meta charset="UTF-8" />
	<meta name="viewport" content="width=device-width" />
	<link rel="icon" type="image/x-icon" href="/favicon.ico" />
	<title>Newsletter</title>
</head>

<body>
	<div class="overflow-auto pt-16 min-h-screen w-full">
		{contents.map(e => <p>{e}</p>)}
	</div>
</body>

</html>
